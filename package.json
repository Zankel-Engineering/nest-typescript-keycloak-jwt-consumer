{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": " if [ -d \"dist\" ]; then rm -Rf dist; fi && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "debug": "nodemon -L --config nodemon-docker-debug.json",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "build:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.3.1",
    "@nestjs/common": "7.0.8",
    "@nestjs/core": "7.0.8",
    "@nestjs/graphql": "7.3.1",
    "@nestjs/microservices": "7.0.8",
    "@nestjs/mongoose": "^6.4.0",
    "@nestjs/passport": "7.0.0",
    "@nestjs/platform-express": "7.0.8",
    "@nestjs/swagger": "4.5.2",
    "@nestjs/terminus": "7.0.1",
    "@nestjs/websockets": "7.0.8",
    "@nestjsplus/cookies": "1.1.1",
    "class-validator": "^0.12.1",
    "cookie-parser": "1.4.5",
    "csurf": "1.11.0",
    "express-rate-limit": "5.1.1",
    "helmet": "3.22.0",
    "jwt-decode": "^2.2.0",
    "keycloak-admin": "1.13.0",
    "keycloak-backend": "^2.0.0",
    "lodash": "4.17.15",
    "logrocket": "1.0.7",
    "moment": "2.24.0",
    "mongoose": "^5.9.10",
    "morgan": "^1.10.0",
    "nestjs-redis": "1.2.7",
    "openid-client": "3.14.2",
    "passport-keycloak-bearer": "2.0.5",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.5",
    "swagger-ui-express": "4.1.4"
  },
  "devDependencies": {
    "@nestjs/testing": "7.0.8",
    "@types/express": "4.17.6",
    "@types/jest": "25.2.1",
    "@types/moment": "2.13.0",
    "@types/mongoose": "^5.7.12",
    "@types/morgan": "1.9.0",
    "@types/node": "12.12.36",
    "@types/supertest": "2.0.8",
    "jest": "25.3.0",
    "nodemon": "2.0.3",
    "prettier": "2.0.4",
    "supertest": "5.0.0-0",
    "ts-jest": "25.4.0",
    "ts-node": "8.8.2",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.15.0",
    "typescript": "3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
